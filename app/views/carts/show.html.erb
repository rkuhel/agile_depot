<% if notice %>
<p id="notice"><%= notice %></p>
<%end%>

<h2>Your Cart </h2>
<ul>
	<% @cart.line_items.each do |item| %>
	<li><%= item.product.title %>
	<span class="cart_img"><%= image_tag(item.product.image_url) %></span> </li> 
	<%end%>
</ul>


<%= link_to 'Edit', edit_cart_path(@cart) %> |
<%= link_to 'Back', carts_path %>
